{
  "expo": {
    "name": "AI/ML Learning Hub",
    "slug": "aiml-learning-hub",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#0f172a"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aiml.learninghub",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.aiml.learninghub",
      "versionCode": 1
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [],
    "platforms": ["ios", "android", "web"]
  }
}
